{
    "manifest_version": 3,
    "name": "SOOP Live Script Injector",
    "version": "1.0",
    "description": "Injects a script into SOOP Live website.",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.sooplive.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}
